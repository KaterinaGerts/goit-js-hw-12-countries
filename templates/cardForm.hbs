<h1 class="country-title">{{name}}</h1>
<ul class="data-list">
  <li class="data-item">Capital:{{capital}}</li>
  <li class="data-item">Population:{{population}}</li>
  <li class="data-item">Languages:
    <ul class="languages">
      {{#each languages}}
      <li class="language">{{this.name}}</li>
      {{/each}}
    </ul>
  </li>
</ul>
<img src="{{flag}}" alt="{{name}}" class="image">